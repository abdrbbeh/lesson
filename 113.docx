class Member:
  def __init__(self, name):
    self.name = name  # Public
one = Member("Ahmed")
print(one.name)
one.name = "Sayed"
print(one.name)

class Member:
  def __init__(self, name):
    self._name = name  # Protected
one = Member("Ahmed")
print(one._name)
one._name = "Sayed"
print(one._name)

class Member:
  def __init__(self, name):
    self.__name = name  # Private
  def say_hello(self):
    return f"Hello {self.__name}"
one = Member("Ahmed")
